{
  "ConnectionStrings": {
    "kuvaConnection": "Server=tcp:{host},1433;Persist Security Info=False;User ID={user};Password={password};MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=True;Connection Timeout=30;Database=Kuva;"
  },
  "ServiceBus": {
    "ConnectionString": "Endpoint=sb://kuvams.servicebus.windows.net/;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey={Azure:BusKey}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ElasticConfiguration": {
    "Uri": "http://localhost:9200",
    "index": "kuva-accounts"
  },
  "AllowedHosts": "*"
}
